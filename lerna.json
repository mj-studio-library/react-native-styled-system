{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "packages": ["packages/*"],
  "version": "0.0.11",
  "npmClient": "yarn",
  "ignoreChanges": ["**/doc/**", "**/example/**"],
  "command": {
    "version": {
      "message": "chore: release packages [skip ci]",
      "conventionalCommits": true,
      "private": false
    },
    "publish": {
      "private": false,
      "registry": "https://registry.npmjs.org/"
    }
  },
  "createRelease": "github"
}
